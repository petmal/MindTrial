    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch MindTrial (home)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}${/}cmd${/}mindtrial${/}main.go",
            "cwd": "${userHome}${/}.${workspaceFolderBasename}${/}",
            "env": {},
            "args": [
                "--config",
                "${userHome}${/}.${workspaceFolderBasename}${/}config.yaml",
                "--html=true",
                "--csv=true",
                "--verbose=true",
                "run",
            ]
        },
        {
            "name": "Launch MindTrial (defaults)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}${/}cmd${/}mindtrial${/}main.go",
            "cwd": "${workspaceFolder}",
            "env": {},
            "args": ["run"],
        },
    ]
}